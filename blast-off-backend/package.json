{
  "name": "blast-off-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development yarn start",
    "prod": "cross-env NODE_ENV=production yarn start",
    "start": "npx yarn-run-all build serve",
    "serve": "node ./dist/bin/www",
    "build": "npx yarn-run-all clean transpile",
    "transpile": "tsc --rootDir stc --outDir dist",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "glob": "^7.1.6",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "mysql": "^2.15.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "typeorm": "^0.2.0",
    "typescript": "^4.1.3",
    "yarn-run-all": "^3.1.1"
  }
}
